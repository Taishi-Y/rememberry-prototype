{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "chrome": false
    },
    "ecmaFeatures": {
        "modules": true,
        "jsx": true
    },
    "rules": {
        "camelcase": 0,
        "eol-last": 0,
        "key-spacing": 0,
        "no-alert": 0,
        "no-debugger": 1,
        "no-loop-func": 0,
        "no-multi-spaces": 0,
        "no-var": 2,
        "no-unused-vars": 1,
        "no-use-before-define": 0,
        "quotes": [ 1, "single", "avoid-escape" ]
    }
}